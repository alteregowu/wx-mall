.login_swrap{
    width: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    align-self: center;
    justify-content: center;
    padding-top: 200rpx;
}
.top_img{
    width: 400rpx;
    height: 200rpx;
    margin-right: 20rpx;
}
.login{
    display: flex;
    flex-direction: column;
    align-self: center;
    align-items: center;
    justify-content: center;
}
.login_line{
    display: flex;
    flex-direction: row;
    align-items: center;
    align-self: center;
    justify-content: center;
    font-size: 20px;
    margin-top: 30rpx;
}
.login_img{
    width: 60rpx;
    height: 60rpx;
    margin-right: 20rpx;
}
.line{
    width: 100%;
    height: 5rpx;
    background-color: #bebcbc;
    margin-top: 10rpx;
}
.btn{
  background-color: #ff6801;
  color: #fff;
  margin-top: 80rpx;
}